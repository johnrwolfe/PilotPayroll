INSERT INTO O_TFR
	VALUES ("b7cf04ef-c030-4379-92eb-89aab2af66c1",
	"56d852da-5807-48de-9bbe-a33dd482bc90",
	'SetStatus',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'LOG::LogInfo( message:"Requesting payroll status update injected: "  + self.newstatus );

if ( self.newstatus == "Approve" )
  send TESTAPP::SubmitPayrollApproval( Department:self.dept );
end if;

if ( self.newstatus == "FinalSubmit" )
  send TESTAPP::SubmitToFinance( Department:self.dept );
end if;

if ( self.newstatus == "Accept" )
  send TESTAPP::Accepted( Department:self.dept );
end if;

select any entry from instances of PayrollEntry
 where ( selected.Edited );
if ( not_empty entry )
  PayrollEntry::SendEntryHolds();
end if;

select one stimulus related by self->Stimulus[R1];
stimulus.Complete();
',
	1,
	'',
	"990f9b1c-4a19-411a-bf44-f6612a67845a",
	0);
